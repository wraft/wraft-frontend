{
  "name": "@wraft/icon",
  "version": "0.0.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "pnpm run icons:create && tsup src/icons/index.ts --format esm,cjs --dts --external react",
    "lint": "eslint . --max-warnings 0",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "icons:create": "npx @svgr/cli --typescript --index-template svgr-index-template.js -d ./src/icons ./rawIcon --config-file ./svgr-config.json --template ./svgr-template.js"
  },
  "devDependencies": {
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@wraft/eslint-config": "*",
    "@wraft/typescript-config": "*",
    "eslint": "^8.56.0",
    "react": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "@svgr/cli": "^8.1.0",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "^20.10.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
